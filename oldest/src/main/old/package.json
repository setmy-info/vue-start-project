    "main": "src/index.js",
    "scripts": {
        "dev": "node src/main/js/vue/build/dev-server.js",
        "start": "npm run dev",
        "www": "node ./src/main/js/www",
        "build": "node src/main/js/vue/build/build.js && hg revert src/main/webapp/WEB-INF",
        "unit": "cross-env BABEL_ENV=test karma start src/test/js/unit/karma.conf.js --single-run",
        "e2e": "node src/test/js/e2e/runner.js",
        "test": "npm run unit && npm run e2e",
        "lint": "eslint --ext .js,.vue src/main/js/vue/src src/test/js/unit/specs src/test/js/e2e/specs"
    },
