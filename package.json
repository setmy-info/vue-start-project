{
    "name": "vue-start-project",
    "version": "0.2.0-SNAPSHOT",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "package": "mv dist $npm_package_name && tar cvzf ./$npm_package_name-$npm_package_version.tar.gz ./$npm_package_name",
        "deploy": "scp $npm_package_name-$npm_package_version.tar.gz $USER_NAME@$HOST_NAME:$DESTINATION",
        "test:unit": "vue-cli-service test:unit",
        "test:e2e": "vue-cli-service test:e2e",
        "lint": "vue-cli-service lint",
        "clear": "rm -Rf $npm_package_name ./$npm_package_name-$npm_package_version.tar.gz"
    },
    "dependencies": {
        "axios": "^0.19.2",
        "vue": "^2.6.11",
        "vue-router": "^3.1.6",
        "vuex": "^3.1.3"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.3.0",
        "@vue/cli-plugin-e2e-nightwatch": "~4.3.0",
        "@vue/cli-plugin-eslint": "~4.3.0",
        "@vue/cli-plugin-router": "~4.3.0",
        "@vue/cli-plugin-unit-mocha": "~4.3.0",
        "@vue/cli-plugin-vuex": "~4.3.0",
        "@vue/cli-service": "~4.3.0",
        "@vue/eslint-config-standard": "^5.1.2",
        "@vue/test-utils": "1.0.0-beta.31",
        "babel-eslint": "^10.1.0",
        "chai": "^4.1.2",
        "chromedriver": "^80.0.1",
        "core-js": "^3.6.4",
        "eslint": "^6.7.2",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.0",
        "eslint-plugin-vue": "^6.2.2",
        "geckodriver": "^1.19.1",
        "less": "^3.0.4",
        "less-loader": "^5.0.0",
        "material-design-icons": "^3.0.1",
        "pug": "^2.0.4",
        "pug-plain-loader": "^1.0.0",
        "vue-template-compiler": "^2.6.11"
    }
}
